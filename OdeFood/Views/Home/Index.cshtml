@model OdeFood.ViewModels.HomeIndexViewModel
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <table>
        @foreach (var restaurant in Model.Restaurants)
        {
            <tr>
                <td>
                    @restaurant.ID
                </td>
                <td>
                    @restaurant.Name
                </td>
            </tr>


        }


    </table>
</body>
</html>